{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "description": "vue-boilerplate",
  "main": "",
  "scripts": {
    "local": "npm run dev --config=local",
    "dev": "webpack-dev-server -d --hot --env.dev --env.config dev",
    "build": "rimraf dist && webpack -p"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-env": "1.2.2",
    "babel-preset-stage-2": "6.22.0",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-config-enough": "0.2.5",
    "eslint-loader": "1.6.1",
    "eslint-plugin-html": "2.0.0",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "postcss-loader": "1.2.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "vue-loader": "10.3.0",
    "vue-template-compiler": "2.1.10",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": {
            "loose": true,
            "modules": false
          }
        }
      ],
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "html"
    ],
    "globals": {
      "DEBUG": false,
      "VERSION": false,
      "CONFIG": false
    }
  },
  "dependencies": {
    "vue": "2.1.10",
    "vue-router": "2.2.0"
  }
}
