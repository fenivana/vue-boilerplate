{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "description": "vue-boilerplate",
  "main": "",
  "scripts": {
    "dev": "webpack-dev-server -d --hot --env.dev",
    "build": "rimraf dist && webpack -p"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-enough": "^0.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "postcss-loader": "^1.2.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "loose": true,
            "modules": false
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "html"
    ],
    "globals": {
      "DEBUG": false,
      "VERSION": false,
      "CONFIG": false
    }
  },
  "dependencies": {
    "vue": "^2.1.6",
    "vue-router": "^2.1.1",
    "vuex": "^2.0.0"
  }
}
